<% @title = "アカウント情報の変更" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @student, url: :student_account do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "更新" %>
      <%= link_to "キャンセル", :student_account %>
    </div>
  <% end %>
</div>
