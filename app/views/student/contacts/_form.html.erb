<div id="generic-form">
  <%= form_with(model: contact, url: student_contacts_path) do |f| %>
    <div>
      <%= f.label :message, "メッセージ", class: "required" %>
      <%= f.text_field :message, required: true %>
    </div>
    <div class="buttons">
      <%= f.submit "送信" %>
    </div>
  <% end %>
</div>

