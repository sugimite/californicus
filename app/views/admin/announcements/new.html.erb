<% @title = "新規お知らせ作成" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @announcement, url: [ :admin, :announcements ] do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "登録" %>
      <%= link_to "キャンセル", :admin_students %>
    </div>
  <% end %>
</div>
